{
  "name": "moneyetdystopie-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "cypress run",
    "test-novid": "cypress run --config video=false",
    "serve-and-test": "start-server-and-test serve http://localhost:8080 test-novid",
    "eslint": "eslint --ext .js,.vue -f json -o target/report.json src"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "vue": "^2.6.12",
    "vue-axios": "^3.2.4",
    "vue-material": "^1.0.0-beta-15"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.5",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^7.1.0",
    "cypress-cucumber-preprocessor": "^4.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "start-server-and-test": "^1.12.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "cypress-cucumber-preprocessor": {
